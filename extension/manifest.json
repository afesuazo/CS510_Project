{
  "name": "Media Recommendation",
  "description": "Provides Media Search Recommendations",
  "version": "1.0",
  "manifest_version": 3,
    "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["*://*.youtube.com/*"]
    }
  ],
  "action": {
    "default_popup": "popup/popup.html"
  }
}
